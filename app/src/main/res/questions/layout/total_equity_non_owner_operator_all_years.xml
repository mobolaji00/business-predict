<merge xmlns:android="http://schemas.android.com/apk/res/android">


    <TextView
        android:id="@+id/total_equity_non_owner_operator_all_years_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What is the total dollar amonut of equity invested in this business by others apart from owners/operators since its start?"
        android:layout_marginLeft="20dp"
        android:tag="@string/question"/>


    <EditText
        android:id="@+id/total_equity_non_owner_operator_all_years"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_marginLeft="20dp"
        android:tag="@string/answer"/>

</merge>