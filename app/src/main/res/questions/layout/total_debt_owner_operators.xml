<merge xmlns:android="http://schemas.android.com/apk/res/android">


    <TextView
        android:id="@+id/total_debt_owner_operator_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What is the total dollar amonut of debt taken on behalf of this business and in the name of the owners/operators?"
        android:layout_marginLeft="20dp"
        android:tag="@string/question"/>


    <EditText
        android:id="@+id/total_debt_owner_operator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_marginLeft="20dp"
        android:tag="@string/answer"/>

</merge>