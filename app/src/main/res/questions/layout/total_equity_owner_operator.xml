<merge xmlns:android="http://schemas.android.com/apk/res/android">


    <TextView
        android:id="@+id/total_equity_owner_operator_question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What is the total dollar amonut of equity invested in this business by owners/operators this year?"
        android:layout_marginLeft="20dp"/>


    <EditText
        android:id="@+id/total_equity_owner_operator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:layout_marginLeft="20dp"
        android:tag="input"/>

</merge>